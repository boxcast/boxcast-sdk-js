  },
  "homepage": "https://github.com/boxcast/boxcast-sdk-js",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "chai": "^4.2.0",
    "eslint": "6.8.0",
