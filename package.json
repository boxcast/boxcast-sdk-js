{
  "name": "boxcast-sdk-js",
  "version": "1.5.2",
  "description": "",
  "main": "lib/boxcast-sdk.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "deploy:latest": "npm run build && aws s3 cp ./lib/boxcast-sdk-latest.min.js s3://js.boxcast.com/libs/boxcast-sdk-latest.min.js --profile=prod-boxcast && aws s3 cp ./lib/boxcast-sdk-latest.min.js.map s3://js.boxcast.com/libs/boxcast-sdk-latest.min.js.map --profile=prod-boxcast",
    "dev": "webpack --progress --watch --env dev",
    "test": "mocha --require babel-register --require jsdom-global/register --file ./test/setup.js ./test/*.spec.js",
    "test:watch": "mocha --require babel-register -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boxcast/boxcast-sdk-js.git"
  },
  "keywords": [],
  "author": "Mike Griffith <mike.griffith@boxcast.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boxcast/boxcast-sdk-js/issues"
  },
  "homepage": "https://github.com/boxcast/boxcast-sdk-js",
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "chai": "^4.3.4",
    "eslint": "7.24.0",
    "eslint-loader": "4.0.2",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mocha": "^10.0.0",
    "qs": "^6.10.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "platform": "^1.3.6"
  }
}
